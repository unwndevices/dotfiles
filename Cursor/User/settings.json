{
  "window.commandCenter": false,
  "editor.fontFamily": "'SF Mono', 'monospace', monospace",
  "editor.fontSize": 14,
  "editor.indentSize": 4,
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "Ros√© Pine",
  "workbench.iconTheme": "rose-pine-icons",
  "workbench.layoutControl.enabled": false,
  "workbench.tree.indent": 18,
  "explorer.confirmDelete": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "files.autoSave": "afterDelay",
  "explorer.confirmDragAndDrop": false,
  "git.openRepositoryInParentFolders": "never",
  "phpCodeSniffer.standard": "Automatic",
  "phpCodeSniffer.autoExecutable": true,
  "phpCodeSniffer.lintAction": "Save",
  "[php]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "obliviousharmony.vscode-php-codesniffer"
  },
  "cursor.cmdk.useThemedDiffBackground": true,
  "workspaceExplorer.workspaceStorageDirectory": "$HOME/dev/",
  "workspaceExplorer.enableCustomIconSearch": false,
  "workbench.editor.showTabs": "none",
  "window.customTitleBarVisibility": "never",
  "extensions.ignoreRecommendations": true,
  "workbench.editor.editorActionsLocation": "hidden",
  "cursor.cpp.enablePartialAccepts": true,
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "native",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  }
}
